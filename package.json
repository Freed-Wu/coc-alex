{
    "name": "alex",
    "version": "0.1.1",
    "displayName": "alex",
    "description": "Find gender favouring, race related, religion inconsiderate or other unequal phrasing",
    "publisher": "shinnn",
    "contributors": [
        "Tobias Lahmann <tobiaslahmann@gmail.com> (https://github.com/tlahmann/)"
    ],
    "repository": "https://github.com/shinnn/vscode-alex",
    "homepage": "https://github.com/shinnn/vscode-alex#readme",
    "bugs": "https://github.com/shinnn/vscode-alex/issues",
    "license": "MIT",
    "icon": "media/icon.svg",
    "galleryBanner": {
        "color": "#FAFAFA",
        "theme": "light"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onLanguage:plaintext",
        "onLanguage:markdown"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "alex configuration options",
            "properties": {
                "alex.enable": {
                    "type": "boolean",
                    "default": false,
                    "description": "Control whether alex automatically check files or not."
                }
            }
        }
    },
    "scripts": {
        "compile": "tsc -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/mocha": "^7.0",
        "@types/node": "^13.9",
        "@types/vscode": "^1.40.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.0",
        "typescript": "^3.7.0",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "alex-vscode": "1.0.0",
        "vscode-languageclient": "6.1.1",
        "vscode-languageserver": "6.1.1"
    }
}
